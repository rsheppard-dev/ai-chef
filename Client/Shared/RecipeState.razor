@using AIChef.Shared

<CascadingValue Value="@this">
    @ChildContent
</CascadingValue>

@code {
    [Parameter]
    public RenderFragment? ChildContent { get; set; }

    public string MealTime { get; set; } = "Breakfast";
    public List<Ingredient> Ingredients { get; set; } = new();
    public List<Idea> Ideas { get; set; } = new();
}
